<logicalNameMap>
    <register name="SingleWord">
        <type>register</type>
        <device>PCIE2</device>
        <register>BOARD.WORD_USER</register>
    </register>
    <register name="FullArea">
        <type>register</type>
        <device>PCIE2</device>
        <register>ADC.AREA_DMAABLE</register>
    </register>
    <register name="PartOfArea">
        <type>range</type>
        <device>PCIE2</device>
        <register>ADC.AREA_DMAABLE</register>
        <index>10</index>
        <length>20</length>
    </register>
    <register name="Channel3">
        <type>channel</type>
        <device>PCIE3</device>
        <register>TEST.NODMA</register>
        <channel>3</channel>
    </register>
    <register name="Channel4">
        <type>channel</type>
        <device>PCIE3</device>
        <register>TEST.NODMA</register>
        <channel>4</channel>
    </register>
    <register name="Constant">
        <type>int_constant</type>
        <value>42</value>
    </register>
    <entry name="Constant2"> <!-- use "entry" tag, which is the deprecated alias for register -->
        <type>int_constant</type>
        <value>666</value>
    </entry>
    <module name="MyModule">
      <module name="SomeSubmodule"> 
        <register name="Variable">
          <type>int_variable</type>
          <value>2</value>
        </register>
      </module>
      <register name="ConfigurableChannel">
        <type>channel</type>
        <device>PCIE3</device>
        <register>TEST.NODMA</register>
        <channel><ref>/MyModule/SomeSubmodule/Variable</ref></channel>
      </register>
    </module>
</logicalNameMap>
